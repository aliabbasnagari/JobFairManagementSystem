<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - JobFairManagementSystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/JobFairManagementSystem.styles.css" asp-append-version="true" />
</head>
<body>
    <partial name="_NavBar" />

    <div class="d-flex h-100 bg-danger">
        <div class="flex-grow-1 bg-dark">
            <h1>Side</h1>
        </div>
        <div >
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
    </div>

  

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - JobFairManagementSystem - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>